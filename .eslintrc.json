{
	"root": true,
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"env": {
		"es2021": true,
		"node": true
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": "off"
	}
}
